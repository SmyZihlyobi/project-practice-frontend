{
  "arrowParens": "avoid",
  "printWidth": 90,
  "trailingComma": "all",
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@/lib",
    "^@/components",
    "^@/app",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
